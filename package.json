    "test": "jest",
    "clean": "watchman watch-del-all &&rm -rf $TMPDIR/react-native-packager-cache-* &&rm -rf $TMPDIR/metro-bundler-cache-* &&rm -rf node_modules/&& npm cache clean --force &&npm install &&npm start -- --reset-cache",
    "log": "adb logcat *:S ReactNative:V ReactNativeJS:V",
    "dp-ios": "xcrun simctl openurl booted",
    "dp-android": "adb shell am start -W -a android.intent.action.VIEW -d",
    "reverse": "adb reverse tcp:8081 tcp:8081",
    "iphone6": "react-native run-ios --simulator 'iPhone 6'",
    "test-android": "cd android && ./gradlew installRelease",
    "release-android": "cd android && ./gradlew assembleRelease",
    "release-ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ./ios/bundle/main.jsbundle --assets-dest ./ios/bundle"
  },
